{
    "openapi": "3.0.0",
    "info": {
      "title": "GIS Data Lake Services API",
      "description": "The GIS Data Lake Services API provides functionality to interact with the GIS Data Lake. The GIS Data Lake is a repository of GIS data that is available for use by the Partner Countries to help with Counter Terrorism efforts. Access to the GIS Data Lake is governored by a set of role-based access control policies.",
      "version": "0.0.1"
    },
    "servers": [
      {
        "url": "http://localhost:5002"
      }
    ],
    "paths": {
      "/items": {
        "get": {
          "summary": "List all items",
          "tags": ["Items"],
          "responses": {
            "200": {
              "description": "A list of items",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Item"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Item": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int64"
            },
            "name": {
              "type": "string"
            }
          }
        }
      }
    }
  }
  